<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.huskyshare.backend.model.user">
	<class name="User" table="tb_user">
		<id name="id">
			<generator class="identity"/>
		</id>
		<property name="username" not-null="true" unique="true" length="50"/>
		<property name="password" not-null="true" length="50"/>
		<property name="firstName" not-null="true" length="50"/>
		<property name="lastName" not-null="true" length="50"/>
		<property name="email" not-null="true" unique="true" length="50"/>
		<property name="mobile" length="16"/>
	</class>
</hibernate-mapping>
