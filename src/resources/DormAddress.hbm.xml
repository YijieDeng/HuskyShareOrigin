<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.huskyshare.backend.model.user">
	<class name="DormAddress" table="tb_dormAddress">
		<id name="id">
			<generator class="identity"/>
		</id>
		<property name="address1" not-null="true" length="255"/>
		<property name="address2" length="255"/>
		<property name="unit" length="32"/>
		<property name="city" not-null="true" length="64"/>
		<property name="state" not-null="true" length="4"/>
		<property name="zip" not-null="true" length="12"/>


	</class>
</hibernate-mapping>
